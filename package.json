{
  "name": "elephant-chrome",
  "private": false,
  "license": "MIT",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently npm:dev:server npm:dev:client",
    "dev:server": "tsx watch --env-file .env ./src-srv/index.ts",
    "dev:client": "concurrently npm:dev:client:*",
    "dev:client:web": "vite",
    "dev:client:css": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build:server": "cd src-srv && tsc",
    "build:client": "tsc && vite build",
    "build:css": "tailwindcss -i ./src/input.css -o ./dist/output.css",
    "build": "npm run build:css && npm run build:client && npm run build:server",
    "start": "node --env-file .env dist/src-srv/index.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tsc": "tsc --noEmit",
    "test": "tsc --noEmit && jest",
    "test:coverage": "jest --coverage",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hocuspocus/extension-database": "^2.7.1",
    "@hocuspocus/extension-logger": "^2.7.1",
    "@hocuspocus/extension-redis": "^2.7.1",
    "@hocuspocus/provider": "^2.7.1",
    "@hocuspocus/server": "^2.7.1",
    "@protobuf-ts/runtime-rpc": "^2.9.1",
    "@protobuf-ts/twirp-transport": "^2.9.1",
    "@tanstack/react-table": "^8.10.7",
    "@ttab/elephant-ui": "^0.0.13",
    "@ttab/textbit": "^0.5.1",
    "class-variance-authority": "^0.7.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "html-entities": "^2.4.0",
    "jose": "^5.0.1",
    "lodash-es": "^4.17.21",
    "node-html-parser": "^6.1.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "redis": "^4.6.10",
    "slate": "^0.100.0",
    "slate-react": "^0.100.0",
    "uuid": "^9.0.1",
    "yjs": "^13.6.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.19",
    "@types/express-ws": "^3.0.2",
    "@types/jest": "^29.5.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pretty-format": "^29.7.0",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.1",
    "tsx": "^4.1.4",
    "typescript": "^5.0.2",
    "vite": "^5.0.0",
    "vite-plugin-static-copy": "^1.0.0"
  }
}
