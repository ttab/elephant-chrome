{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "../dist/src-srv",
    "skipLibCheck": true,
    "paths": {
      "@/shared/*": [
        "../shared/*"
      ],
      "@/shared": [
        "../shared/index.ts"
      ],
      "@/protos/*": [
        "../shared/protos/*"
      ]
    }
  }
}
